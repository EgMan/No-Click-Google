{
  "manifest_version": 2,
  "name": "No click google",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*",
        "http://www.google.com/search*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
